      var input = $flag
      var hashed = CryptoJS.SHA512(input).toString(CryptoJS.enc.Base64);

      $success = "kl4L9GzyfPccA7tMeEf+vidocY6v7TXtJGpeNaWAYmXko3QoDAVREopOOEqdba5pjr6IHfrxNcnx95ZyaJNQ4Q==" == hashed
